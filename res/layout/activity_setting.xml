<?xml version="1.0" encoding="utf-8"?>
<TabHost xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/TabHost01"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" 
    android:background="@drawable/common_bg"
    >
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical" >

        <TabWidget
            android:id="@android:id/tabs"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" />

        <FrameLayout
            android:id="@android:id/tabcontent"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" 
            android:background="@drawable/list_item"
            >

            <LinearLayout
                android:id="@+id/LinearLayout1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" 
                android:gravity="center_horizontal"
                android:orientation="vertical"
                >
				
                <Button
                    android:id="@+id/btn_personal_account"
                    android:layout_width="130px"
                    android:layout_height="40px"
                    android:text="个人资料" 
                    android:textSize="18px"
                    android:gravity="center"
                    android:textColor="@color/black"
                    android:background="@drawable/btn_bg_common"
                    android:layout_marginTop="10px"
                    />
                 <Button
                    android:id="@+id/btn_personal_security"
                    android:layout_width="130px"
                    android:layout_height="40px"
                    android:text="账户安全" 
                    android:textSize="18px"
                    android:gravity="center"
                    android:textColor="@color/black"
                    android:background="@drawable/btn_bg_common"
                    android:layout_marginTop="10px"
                    />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/LinearLayout2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" 
                android:orientation="vertical"
                android:gravity="center_horizontal"
                >

                <Button
                    android:id="@+id/btn_hy_da"
                    android:layout_width="130px"
                    android:layout_height="40px"
                    android:text="还原档案" 
                    android:textSize="18px"
                    android:gravity="center"
                    android:textColor="@color/black"
                    android:layout_marginTop="10px"
                    android:background="@drawable/btn_bg_common"
                    />
                <Button
                    android:id="@+id/btn_bf_st"
                    android:layout_width="130px"
                    android:layout_height="40px"
                    android:text="备份手图" 
                    android:textSize="18px"
                    android:gravity="center"
                    android:textColor="@color/black"
                    android:background="@drawable/btn_bg_common"
                    android:layout_marginTop="10px"
                    />
                <Button
                    android:id="@+id/btn_hy_st"
                    android:layout_width="130px"
                    android:layout_height="40px"
                    android:text="还原手图" 
                    android:gravity="center"
                    android:textSize="18px"
                    android:textColor="@color/black"
                    android:background="@drawable/btn_bg_common"
                    android:layout_marginTop="10px"
                    />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/LinearLayout3"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" 
                android:orientation="vertical"
                android:gravity="center_horizontal"
                >
				 <Button
                    android:id="@+id/btn_zx_gl"
                    android:layout_width="130px"
                    android:layout_height="40px"
                    android:text="咨询管理" 
                    android:gravity="center"
                    android:textSize="18px"
                    android:textColor="@color/black"
                    android:layout_marginTop="10px"
                    android:background="@drawable/btn_bg_common"
                    />
                <Button
                    android:id="@+id/btn_ts_gl"
                    android:layout_width="130px"
                    android:layout_height="40px"
                    android:text="投诉管理" 
                    android:textSize="18px"
                    android:gravity="center"
                    android:textColor="@color/black"
                    android:background="@drawable/btn_bg_common"
                    android:layout_marginTop="10px"
                    />
                <Button
                    android:id="@+id/btn_tz_gl"
                    android:layout_width="130px"
                    android:layout_height="40px"
                    android:text="通知管理" 
                    android:gravity="center"
                    android:textSize="18px"
                    android:textColor="@color/black"
                    android:background="@drawable/btn_bg_common"
                    android:layout_marginTop="10px"
                    />                
            </LinearLayout>

            <LinearLayout
                android:id="@+id/LinearLayout4"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" 
                android:orientation="vertical"
                android:gravity="center_horizontal"
                >
				<Button
                    android:id="@+id/btn_4_czlc"
                    android:layout_width="130px"
                    android:layout_height="40px"
                    android:text="操作流程" 
                    android:gravity="center"
                    android:textSize="18px"
                    android:textColor="@color/black"
                    android:background="@drawable/btn_bg_common"
                    android:layout_marginTop="10px"
                    />  
                 <Button
                    android:id="@+id/btn_4_hydl"
                    android:layout_width="130px"
                    android:layout_height="40px"
                    android:text="会员登录" 
                    android:textSize="18px"
                    android:gravity="center"
                    android:textColor="@color/black"
                    android:background="@drawable/btn_bg_common"
                    android:layout_marginTop="10px"
                    />  
                 <Button
                    android:id="@+id/btn_4_szdh"
                    android:layout_width="130px"
                    android:layout_height="40px"
                    android:text="手诊导航" 
                    android:textSize="18px"
                    android:gravity="center"
                    android:textColor="@color/black"
                    android:background="@drawable/btn_bg_common"
                    android:layout_marginTop="10px"
                    /> 
                    <Button
	                    android:id="@+id/btn_4_tszx"
	                    android:layout_width="130px"
	                    android:layout_height="40px"
	                    android:text="投诉中心" 
	                     android:gravity="center"
	                    android:textSize="18px"
	                    android:textColor="@color/black"
	                    android:background="@drawable/btn_bg_common"
	                    android:layout_marginTop="10px"
	                    />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/LinearLayout5"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" 
                android:orientation="vertical"
                android:gravity="center_horizontal"
                >

               <Button
                    android:id="@+id/btn_4_au"
                    android:layout_width="130px"
                    android:layout_height="40px"
                    android:text="康客授权" 
                    android:gravity="center"
                    android:textSize="18px"
                    android:textColor="@color/black"
                    android:background="@drawable/btn_bg_common"
                    android:layout_marginTop="10px"
                    />
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>

</TabHost><!--
